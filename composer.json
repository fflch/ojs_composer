{
  "name": "usp/moodle",
  "require": {
    "pkp/ojs": "3.4.0.8"
  },
  "extra": {
    "installerdir": "ojs"
  },
  "autoload": {
    "psr-4": {
      "OjsComposer\\": "src"
    }
  },
  "scripts": {
    "post-install-cmd": "OjsComposer\\OjsComposer::postInstall",
    "post-update-cmd": "OjsComposer\\OjsComposer::postInstall"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
